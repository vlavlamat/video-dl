{
  "name": "vmatkovskii/video-dl",
  "description": "Simple terminal tool to download and re-encode videos using yt-dlp and ffmpeg",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Matkovskii",
      "email": "vlavlamat@icloud.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "VMatkovskii\\VideoDl\\": "src/"
    }
  },
  "bin": [
    "bin/video-dl"
  ],
  "require": {
    "php": ">=8.0",
    "ext-fileinfo": "*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "homepage": "https://github.com/vmatkovskii/video-dl",
  "keywords": ["cli", "video", "downloader", "yt-dlp", "ffmpeg", "composer package"]
}
